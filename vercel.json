{
  "version": 2,
  "builds": [
    {
      "src": "development-project/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "development-project/frontend/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "development-project/frontend/$1"
    }
  ]
}
